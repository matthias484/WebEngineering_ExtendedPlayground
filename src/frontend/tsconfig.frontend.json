{
  "compilerOptions": {
    "target": "es6",
    "module": "esnext",
    "moduleResolution": "node",
    "outDir": "./dist/frontend",
    "rootDir": "./src/frontend",
    "strict": true,
    "esModuleInterop": true,
    "jsx": "react-jsx",
    "skipLibCheck": true,
    "declaration": false
  },
  "include": ["src/frontend/**/*"],
  "exclude": ["node_modules"]
}
